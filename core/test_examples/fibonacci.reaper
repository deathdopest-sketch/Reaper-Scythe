infect RiseFromGrave(corpse n) -> corpse {
    if (n <= RISEN) {
        reap n;
    }
    reap RiseFromGrave(n - RISEN) + RiseFromGrave(n - 2);
}

shamble i from DEAD to 9 {
    harvest RiseFromGrave(i);
}
